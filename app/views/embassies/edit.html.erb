<%= form_with model: @embassy do |form| %>
  <%= form.label :building, "Bâtiment" %>
  <%= form.text_field :building %> <br>
  
  <%= form.label :latitude, "Latitude" %>
  <%= form.text_field :latitude %> <br>

  <%= form.label :longitude, "Longitude" %>
  <%= form.text_field :longitude %> <br>

  <%= form.label :contact, "Coordonnées" %>
  <%= form.text_field :contact %> <br>

  <%= form.submit "Modifier" %>
<% end %>