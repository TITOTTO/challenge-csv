<tr>
  <td><%= @country.country_fr%></td>
  <td><%= @country.country_en%></td>
  <td><%= @country.iso_numerical%></td>
  <td><%= @country.iso_alpha2%></td>
  <td><%= @country.iso_alpha3%></td>
  <% if @country.embassy? %>
  <td><%= @country.embassy.contact%></td>
  <% else %>
  <td>Il n'y a pas d'ambassade dans ce pays</td>
  <% end %>
</tr>


<% if @country.french_foreigners? %>
  <tr>
    <td> Année </td>
    <td> Nombre de français </td>
  </tr>
  <% @country.french_foreigners.each do |french_foreigner| %>
  <tr>
    <td><%= french_foreigner.year%></td>
    <td><%= french_foreigner.nbr_fr%></td>
  </tr>
  <% end %>
<% else %>
  <td>Il n'y a pas de français dans ce pays</td>
<% end %>